{
  "name": "vehicle-organiser",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.7",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^5.3.2",
    "concurrently": "^8.2.1",
    "env-cmd": "^10.1.0",
    "json-server": "^0.17.4",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.16.0",
    "react-scripts": "5.0.1",
    "reactstrap": "^9.2.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "serve-json": "json-server --watch database/vehicle-organiser-db.json --port 4000  --host 0.0.0.0 ",
    "start:dev": "env-cmd -f .env.dev react-scripts start",
    "start:prod": "env-cmd -f .env.prod react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently \"npm run serve-json\" \"npm run start:dev\"",
    "prod": "concurrently \"npm run serve-json\" \"npm run start:prod\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
